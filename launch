#!/usr/bin/env python3
# -*- coding: utf-8 -*-

def main():
    from sima import core
    from sima.plugins.crop import Crop
    m = core.Sima()
    m.register_plugin(Crop)
    try:
        m.run()
    except KeyboardInterrupt:
        m.shutdown()


# Script starts here
if __name__ == '__main__':
    main()


# VIM MODLINE
# vim: ai ts=4 sw=4 sts=4 expandtab
